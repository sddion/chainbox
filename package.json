{
  "name": "@sddion/chainbox",
  "version": "1.0.2",
  "description": "Execution-first backend framework for Next.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sddion/chainbox.git"
  },
  "keywords": [
    "nextjs",
    "backend",
    "framework",
    "execution",
    "serverless",
    "typescript"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "bin": {
    "chainbox": "./dist/bin/Chainbox.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.0.0",
    "commander": "^14.0.2",
    "jiti": "^2.6.1",
    "jose": "^6.1.3",
    "undici": "^7.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "react-native": "./dist/client/index.js",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.js"
    },
    "./node": {
      "types": "./dist/node/Server.d.ts",
      "import": "./dist/node/Server.js",
      "require": "./dist/node/Server.js"
    },
    "./transport": {
      "types": "./dist/transport/Mesh.d.ts",
      "import": "./dist/transport/Mesh.js",
      "require": "./dist/transport/Mesh.js"
    },
    "./core/Adapter": {
      "types": "./dist/core/Adapter.d.ts",
      "import": "./dist/core/Adapter.js",
      "require": "./dist/core/Adapter.js"
    }
  }
}