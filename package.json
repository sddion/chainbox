{
  "name": "@sddion/chainbox",
  "version": "1.0.6",
  "description": "Execution-first backend framework for Next.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sddion/chainbox.git"
  },
  "keywords": [
    "nextjs",
    "backend",
    "framework",
    "execution",
    "serverless",
    "typescript",
    "rpc-less",
    "distributed-execution",
    "capabilities",
    "zero-trust",
    "security-by-deletion",
    "api-less",
    "mesh",
    "hono",
    "vite",
    "express"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "bin": {
    "chainbox": "./dist/bin/Chainbox.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run build:browser",
    "build:browser": "esbuild client/Browser.ts --bundle --minify --outfile=dist/chainbox.min.js --format=iife --global-name=Chainbox && esbuild client/Browser.ts --bundle --outfile=dist/chainbox.js --format=iife --global-name=Chainbox",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.0.0",
    "@types/dotenv": "^6.1.1",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "firebase-admin": "^13.6.0",
    "jiti": "^2.6.1",
    "jose": "^6.1.3",
    "undici": "^7.16.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/connect": "^3.4.38",
    "@types/express": "^5.0.6",
    "@types/express-serve-static-core": "^5.1.0",
    "@types/http-errors": "^2.0.5",
    "@types/mime": "^3.0.4",
    "@types/node": "^20.0.0",
    "@types/qs": "^6.14.0",
    "@types/range-parser": "^1.2.7",
    "@types/send": "^1.2.1",
    "@types/serve-static": "^2.2.0",
    "esbuild": "^0.27.2",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.3.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "react-native": "./dist/client/index.js",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.js"
    },
    "./node": {
      "types": "./dist/node/Server.d.ts",
      "import": "./dist/node/Server.js",
      "require": "./dist/node/Server.js"
    },
    "./vite": {
      "types": "./dist/integrations/vite.d.ts",
      "import": "./dist/integrations/vite.js",
      "require": "./dist/integrations/vite.js"
    },
    "./express": {
      "types": "./dist/integrations/express.d.ts",
      "import": "./dist/integrations/express.js",
      "require": "./dist/integrations/express.js"
    },
    "./fastify": {
      "types": "./dist/integrations/fastify.d.ts",
      "import": "./dist/integrations/fastify.js",
      "require": "./dist/integrations/fastify.js"
    },
    "./hono": {
      "types": "./dist/integrations/hono.d.ts",
      "import": "./dist/integrations/hono.js",
      "require": "./dist/integrations/hono.js"
    },
    "./next": {
      "types": "./dist/integrations/next.d.ts",
      "import": "./dist/integrations/next.js",
      "require": "./dist/integrations/next.js"
    },
    "./transport": {
      "types": "./dist/transport/Mesh.d.ts",
      "import": "./dist/transport/Mesh.js",
      "require": "./dist/transport/Mesh.js"
    },
    "./core/Adapter": {
      "types": "./dist/core/Adapter.d.ts",
      "import": "./dist/core/Adapter.js",
      "require": "./dist/core/Adapter.js"
    }
  }
}
